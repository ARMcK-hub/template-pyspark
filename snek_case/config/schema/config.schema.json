{
    "$schema": "http://json-schema.org/schema",
    "title": "template-pyspark Config",
    "description": "template-pyspark Configuration Schema",
    "type": "object",
    "required": [
        "app_name",
        "snek_type"
    ],
    "properties": {
        "app_name": {
            "type": "string",
            "default": "snek_case",
            "description": "Name of this application, used at run-time setup"
        },
        "snek_type": {
            "type": "string",
            "default": "cool",
            "description": "Type of Snek to print",
            "examples": [
                "cool",
                "normal"
            ]
        }
    }
}